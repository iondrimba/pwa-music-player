  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "fast-average-color": "^6.2.0",
    "history": "^5.0.0",
    "node-sass": "^4.14.1",
    "npm-force-resolutions": "^0.0.3",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.13.1",
    "react-loadable": "^5.5.0",
    "react-scripts": "^4.0.0",
    "react-test-renderer": "^16.14.0"
